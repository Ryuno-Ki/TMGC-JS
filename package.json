{
  "name": "TMGC",
  "version": "0.1.0",
  "description": "A little Tamagotchi Simulator for Firefox OS",
  "main": "/src/index.js",
  "scripts": {
    "build": "NODE_ENV=PROD rollup -c rollup.config.prod.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "rollup:dev": "NODE_ENV=DEV rollup -w -c rollup.config.dev.js",
    "rollup:test": "NODE_ENV=TEST rollup -c rollup.config.test.js",
    "pretest": "npm run rollup:test",
    "start": "npm run rollup:dev",
    "test": "mocha",
    "test:coverage": "istanbul check-coverage --statements 85 --branches 70 --functions 100 --lines 90"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Ryuno-Ki/TMGC-JS.git"
  },
  "keywords": [
    "TMGC",
    "Firefox OS",
    "App"
  ],
  "author": "Richard Armuelles, Andr√© Jaenisch",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Ryuno-Ki/TMGC-JS/issues"
  },
  "homepage": "https://github.com/Ryuno-Ki/TMGC-JS",
  "devDependencies": {
    "@fortawesome/fontawesome-free-solid": "5.0.1",
    "chai": "4.1.2",
    "chai-string": "1.4.0",
    "choo-devtools": "2.2.0",
    "http-server": "0.10.0",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "rollup": "0.52.1",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-copy": "0.2.3",
    "rollup-plugin-eslint": "4.0.0",
    "rollup-plugin-istanbul": "2.0.0",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-scss": "0.4.0",
    "rollup-plugin-serve": "0.4.2",
    "rollup-plugin-url": "1.3.0",
    "standard": "10.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "1.0.1",
    "choo": "6.6.0"
  }
}
