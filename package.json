{
  "name": "TMGC",
  "version": "0.1.0",
  "description": "A little Tamagotchi Simulator for Firefox OS",
  "main": "/src/index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "rollup:dev": "NODE_ENV=DEV rollup -c -w",
    "rollup:test": "NODE_ENV=TEST rollup -c",
    "prestart": "npm run rollup:dev",
    "pretest": "npm run rollup:test",
    "start": "http-server dist",
